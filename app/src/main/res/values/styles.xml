<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/bottom_lable_color</item>
        <item name="colorPrimaryDark">@color/bottom_lable_color</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <!--自定义的checkbox样式,此样式作废-->
    <style name="CustomCheckboxTheme" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/mycheckbox</item>
    </style>


    <!--自定义的全局样式，继承自appcompat-->
    <style name="MyAppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/bottom_lable_color</item>
        <item name="colorPrimaryDark">@color/bottom_lable_color</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <!--popu的进出的样式-->
    <style name="popwin_anim_style">
        <item name="android:windowEnterAnimation">@anim/popu_in</item>
        <item name="android:windowExitAnimation">@anim/popu_out</item>
    </style>

    <!--别人自定义dialog的样式-->
    <style name="custom_dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.6</item>
    </style>



    <!-- 自定义loading dialog -->
    <style name="loading_dialog" parent="android:style/Theme.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowBackground">@android:color/transparent</item>  
      <item name="android:backgroundDimEnabled">false</item> 
    </style>

    <!--导航栏relativelayout的样式-->
    <style name="dao_hang_relative_layout_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/bottom_lable_color</item>
        <item name="android:paddingTop">@dimen/head_padding</item>
        <item name="android:paddingBottom">@dimen/head_padding</item>
    </style>

    <!--导航栏文字的样式-->
    <style name="dao_hang_text_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:padding">5dp</item>
        <item name="android:textSize">@dimen/dao_hang_base_font</item>
        <item name="android:textColor">@color/white_my</item>
    </style>


    <!--导航栏返回键的样式-->
    <style name="dao_hang_img_style">
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:src">@mipmap/leftdaohang</item>
        <item name="android:layout_marginLeft">5dp</item>
    </style>


    <!--mainActivity底部标签linearlayout的样式-->
    <style name="main_activity_bottom_label_ll_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center</item>
    </style>


    <!--mainActivity底部标签imageview的样式-->
    <style name="main_activity_bottom_label_img_style">
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">20dp</item>
    </style>


    <!--mainActivity底部标签textview的样式-->
    <style name="main_activity_bottom_label_text_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/main_page_bottom_biao_qian_size</item>
    </style>


    <!--homefragment八个按钮文字的样式-->
    <style name="home_fragment_eight_text_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:textSize">@dimen/fragment_eight_font_size</item>
        <item name="android:textColor">@color/myblack</item>
    </style>


    <!--精品推荐和店铺街标题的样式-->
    <style name="jing_ping_and_dian_pu_biao_ti_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/myblack</item>
        <item name="android:layout_centerVertical">true</item>
    </style>


    <!--商品详情里面运费地区等的样式-->
    <style name="product_detail_font_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/product_detail_base_font_size</item>
        <item name="android:textColor">@color/myblack</item>
        <item name="android:textStyle">normal</item>
    </style>


    <!--弹出关闭按钮的样式-->
    <style name="tan_chu_close">
        <item name="android:layout_width">25dp</item>
        <item name="android:layout_height">25dp</item>
        <item name="android:src">@mipmap/close</item>
    </style>


    <!--搜索栏的布局的样式-->
    <style name="search_ll_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:focusable">true</item>
        <item name="android:focusableInTouchMode">true</item>
        <item name="android:background">@color/bottom_lable_color</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:paddingRight">10dp</item>
    </style>

    <!--搜索栏imageview的样式-->
    <style name="search_img_dao_hang_style">
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:background">@mipmap/leftdaohang</item>
    </style>

    <!--搜索栏搜索框的样式-->
    <style name="search_img_search_kuang_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:src">@mipmap/search_pic</item>
    </style>

    <!--搜索结果综合排序的文字的样式-->
    <style name="search_result_text_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/search_result_text_size</item>
    </style>

    <!--搜索结果综合排序的图片的样式-->
    <style name="search_result_img_style">
        <item name="android:layout_width">@dimen/search_result_icon_size</item>
        <item name="android:layout_height">@dimen/search_result_icon_size</item>
        <item name="android:src">@mipmap/down_black_sort</item>
    </style>


    <!--店铺主页和店铺详情商品数量的样式（全部，新品和折扣）-->
    <style name="shop_home_good_count_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/myblack</item>
        <item name="android:layout_gravity">center</item>
    </style>


    <!--店铺详情的描述信息的样式（如电话，地址，店铺名称之类的）-->
    <style name="shop_detail_desc_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/myblack</item>
    </style>


    <!--个人中心标题的样式-->
    <style name="personal_center_title_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/myblack</item>
    </style>


    <!--资料设置的小标题的样式-->
    <style name="data_set_title_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/myblack</item>
    </style>


    <!--侧滑relayout的样式-->
    <style name="menu_re_layout_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">10dp</item>
    </style>


    <!--侧滑imageview的样式-->
    <style name="menu_img_style">
        <item name="android:layout_width">25dp</item>
        <item name="android:layout_height">25dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:clickable">false</item>
    </style>


    <!--侧滑textview的样式-->
    <style name="menu_text_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">10dp</item>
    </style>


    <!--商品名称的样式-->
    <style name="good_name_base_style">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">left</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/myblack</item>
        <item name="android:text">测试</item>
        <item name="android:layout_marginRight">5dp</item>
    </style>


    <!--商品价格的样式-->
    <style name="good_price_base_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">left</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/bottom_lable_color</item>
        <item name="android:text">0.00</item>
    </style>


    <!--商品名称隔左边5dp的样式-->
    <style name="good_name_style_left_five" parent="good_name_base_style">
        <item name="android:layout_marginLeft">5dp</item>
    </style>


    <!--商品价格隔左边5dp的样式-->
    <style name="good_price_style_left_five" parent="good_price_base_style">
        <item name="android:layout_marginLeft">5dp</item>
    </style>


    <!--分类的linearlayout的样式-->
    <style name="category_ll_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:paddingBottom">10dp</item>
    </style>


    <!--分类的textview的样式-->
    <style name="category_textview_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_gravity">left</item>
        <item name="android:textSize">16sp</item>
        <item name="android:paddingBottom">10dp</item>
    </style>


    <!--分类的mygridview的样式-->
    <style name="category_gridview_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:numColumns">3</item>
        <item name="android:verticalSpacing">10dp</item>
        <item name="android:horizontalSpacing">10dp</item>
        <item name="android:gravity">center</item>
    </style>


    <!--个人中心待付款的imagview样式-->
    <style name="personal_center_dai_fu_kuan_imgview_style">
        <item name="android:layout_width">26dp</item>
        <item name="android:layout_height">26dp</item>
        <item name="android:layout_gravity">center</item>
    </style>


    <!--个人中心待付款的textview样式-->
    <style name="personal_center_dai_fu_kuan_text_view_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
    </style>


    <!--申请记录详情的字体的样式-->
    <style name="shen_qing_record_detail_text_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/myblack</item>
    </style>


    <!--提现和充值的字体样式-->
    <style name="ti_xian_text_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/myblack</item>
    </style>


    <!--添加银行卡的字体样式-->
    <style name="add_bank_card_text_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/myblack</item>
    </style>


</resources>
